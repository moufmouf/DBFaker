{
  "autoload-dev": {
    "psr-4": { "DBFaker\\": "tests/" }
  },
  "autoload": {
    "psr-4": { "DBFaker\\": "src/" }
  },
  "require": {
    "fzaninotto/faker": "^1.6",
    "doctrine/dbal": "^2.5",
    "symfony/console": "^3.3",
    "psr/log": "^1.0",
    "php": ">=7.1"
  },
  "require-dev" : {
    "mouf/utils.log.psr.errorlog_logger": "^2.0",
    "phpunit/phpunit": "^6.2",
    "satooshi/php-coveralls": "^1",
    "phpstan/phpstan": "^0.7",
    "thecodingmachine/phpstan-strict-rules": "^0.7.3"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -c phpstan.neon --level=4 --no-progress -vvv"
  },
  "minimum-stability" : "dev",
  "prefer-stable": true
}
